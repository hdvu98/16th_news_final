 {{#section 'css'}}
       <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
{{/section}} 
{{#section 'js'}}
        <!-- search -->
        <script>
                $(document).ready(function(){
                  $("#searchInput").on("keyup", function() {
                    var value = $(this).val().toLowerCase();
                    $("#managementTable tr").filter(function() {
                      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                    });
                  });
                });
             </script>
        <!-- end search -->

    <script>
            $("#menu-toggle").click(function(e) {
                e.preventDefault();
                $("#wrapper").toggleClass("toggled");
              });
    </script>     
    
 {{/section}}


  <div class="d-flex" id="wrapper">

        <!-- Sidebar -->
        <div class="bg-dark border-right" id="sidebar-wrapper">
          <div class="sidebar-heading"><i class="fa fa-dashboard"></i> Dashboard </div>
          <div class="list-group list-group-flush">
            <a href="/powerful/adminCateMag" class="list-group-item list-group-item-action bg-dark">Categories Management</a>
            <a href="/powerful/adminTagMag" class="list-group-item list-group-item-action bg-dark">Tags Management</a>
            <a href="/powerful/adminMemberMag" class="list-group-item list-group-item-action bg-dark">Member Management</a>
            <a href="/powerful/adminWriterMag" class="list-group-item list-group-item-action bg-dark">Writer Management</a>
            <a href="/powerful/adminEditorMag" class="list-group-item list-group-item-action bg-dark">Editor Management</a>
            <a href="/powerful/adminPostMag" class="list-group-item list-group-item-action bg-dark">Post Management</a>

            
          </div>
        </div>
        <!-- /#sidebar-wrapper -->
    
        <!-- Page Content -->
        <div id="page-content-wrapper">
    
          <nav class="navbar navbar-expand-lg navbar-light bg-light border-bottom">
            <button class="btn tag" id="menu-toggle"><i class="fa fa-navicon"></i></button>
    
            <!-- <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button> -->

          </nav>
    
           <div class="card shadow">
             {{#if this.error}}
    <div class="alert alert-danger" role="alert">
      Invalid categoryId.
      <a href="/powerful/adminCateMag" class="alert-link">Go Back</a>
    </div>
    {{else}}
  <h4 class="card-header">
    {{nameCategory.Name_parentscate}} <i><a class="btn btn-primary" href="/powerful/adminCateMag" role="button" style="float:right;">
        <i class="fa fa-backward" aria-hidden="true"></i>
        Back To List
      </a></i>
  </h4>
  <div >
        <label for="txtCatID">#</label>
        <input readonly type="text" class="form-control" id="txtCatID" name="IDCate_Parents" value="{{nameCategory.IDCate_Parents}}">
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <th class="text-center" scope="col">#</th>
        <th scope="col">Topics</th>
        <th scope="col" class="text-right">
          
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each this.topics}}
      <tr>
        <th class="text-center" scope="row">{{IDCate_Child}}</th>
        <td>{{Name_childcate}}</td>
        <td class="text-right">
           
          <a class="btn btn-primary btn-sm" href="/powerful/editTopics/{{IDCate_Child}}" role="button">
            
            <i class="fa fa-pencil" aria-hidden="true"></i>
          </a>

        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  {{/if}}
</div>

        </div>
        <!-- /#page-content-wrapper -->
    
      </div>
    
      
  

      <!-- /#wrapper -->   