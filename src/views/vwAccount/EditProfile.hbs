 {{#section 'css'}}
       <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
{{/section}} 
 {{#section 'js'}}
 <script
  src="https://cdnjs.cloudflare.com/ajax/libs/jquery-datetimepicker/2.5.20/jquery.datetimepicker.full.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.0/jquery.validate.min.js"></script>
    <script>
  $(function () {

  $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
      });
    $('#frmRegister').validate({
      rules: {
      
        
        email: {
          required: true,
          email: true
        },
       
        phone: {
          required: true,
        },
      },
      messages: {
       
        
        email: {
          required: 'Please enter a valid email address.',
          email: 'Please enter a valid email address.'
        },
       
         phone: {
          required: 'Valid phone is required.',
        }
      },

      errorElement: 'small',
      errorClass: 'help-block text-danger',
      validClass: 'is-valid',
      highlight: function (e) {
        $(e).removeClass('is-valid').addClass('is-invalid');
      },
      unhighlight: function (e) {
        $(e).removeClass('is-invalid').addClass('is-valid');
      }
    });
  })
</script>
    
 {{/section}}

    <div class="d-flex" id="wrapper">

            <!-- Sidebar -->
            <div class="bg-dark border-right" id="sidebar-wrapper">
              <div class="sidebar-heading"><i class="fa fa-dashboard"></i> Dashboard </div>
              <div class="list-group list-group-flush">
                <a href="/account/profile" class="list-group-item list-group-item-action bg-dark">Personal Information</a>
                <a href="/account/changePassword" class="list-group-item list-group-item-action bg-dark">Change Password</a>
                
              </div>
            </div>
            <!-- /#sidebar-wrapper -->
        
            <!-- Page Content -->
            <div id="page-content-wrapper">
        
              <nav class="navbar navbar-expand-lg navbar-light bg-light border-bottom">
                <button class="btn tag" id="menu-toggle"><i class="fa fa-navicon"></i></button>
        
                <!-- <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                  <span class="navbar-toggler-icon"></span>
                </button> -->

              </nav>
        
              <div class="container-fluid">
                    <div class="w3layoutscontaineragileits" style="margin-right: 20%;width: 50%">
                          
                               
                           
                                <div class="section-heading">
                                    <h5 class="header-area">Contact Information</h5>
                                </div>
                                <form method="POST" id="frmRegister" novalidate >
                                <div class="form-group">
                                     <label style="float: left;">Email: </label>
                                    <input type="email" class="form-control" name="email" id="exampleInputEmail1" placeholder="abc@example.com" value="{{this.inforUser.Email}}">
                                </div>
                                <div class="form-group">
                                     <label style="float: left;">Phone: </label>
                                    <input type="text" class="form-control" name="phone" id="exampleInputPhone" value="{{this.inforUser.Phone}}" placeholder="Phone number" >
                                </div>
                                <button type="submit" class="btn btn-lg btn-success btn-block">
                                  <i class="fa fa-check" aria-hidden="true"></i>
                                  Save
                                </button>
                            </form>
                            </div>
                  



              </div>
            </div>
            <!-- /#page-content-wrapper -->
        
          </div>
          <!-- /#wrapper -->   
   
